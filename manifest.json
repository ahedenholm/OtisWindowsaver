{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": [
    "tabs",
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo16px.png",
      "32": "images/logo32px.png",
      "48": "images/logo48px.png",
      "128": "images/logo128px.png"
    }
  },
  "icons": {
    "16": "images/logo16px.png",
    "32": "images/logo32px.png",
    "48": "images/logo48px.png",
    "128": "images/logo128px.png"
  },
  "manifest_version": 2
}