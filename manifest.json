{
  "name": "Otis Windowsaver",
  "short_name": "Windowsaver",
  "version": "1.3.0",
  "description": "Otis will help you save and load the positions and sizes of your browser windows.",
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "popup.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo16px.png",
      "32": "images/logo32px.png",
      "48": "images/logo48px.png",
      "128": "images/logo128px.png"
    }
  },
  "commands": {
    "OpenPreset1": {
      "description": "Open preset 1"
    },
    "OpenPreset2": {
      "description": "Open preset 2"
    },
    "OpenPreset3": {
      "description": "Open preset 3"
    },
    "OpenPreset4": {
      "description": "Open preset 4"
    },
    "OpenPreset5": {
      "description": "Open preset 5"
    }
  },
    "icons": {
      "16": "images/logo16px.png",
      "32": "images/logo32px.png",
      "48": "images/logo48px.png",
      "128": "images/logo128px.png"
    },
    "manifest_version": 2
  }